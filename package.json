{
  "name": "ui-client",
  "version": "1.1.0",
  "description": "UI for Customer",
  "main": "dist/ui-client.bundle",
  "typings": "dist/ui-client.bundle",
  "scripts": {
    "start": "npm-run-all -p dev:api dev:app",
    "test": "ava",
    "coverage": "nyc npm test",
    "clean": "rimraf dist",
    "watch": "webpack --watch --progress --colors",
    "build": "npm-run-all -s clean build:dev",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:profile": "mkdirp dist && webpack --profile --json > dist/stats.json",
    "build:ci": "npm install",
    "dev:api": "nodemon --watch server/* server",
    "dev:app": "webpack-dev-server --mode development --open",
    "lint:fix": "eslint --fix",
    "prepare": "npm-run-all -s clean build:prod test",
    "release": "release",
    "tsc": "tsc -p . -w"
  },
  "ava": {
    "source": [
      "**/*.{js}",
      "!dist/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "@angular/animations": "~7.0",
    "@angular/common": "~7.0",
    "@angular/compiler": "~7.0",
