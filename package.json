    "@angular/forms": "~7.0",
    "@angular/http": "~7.0",
    "@angular/platform-browser": "~7.0",
    "@angular/platform-browser-dynamic": "~7.0",
    "@angular/platform-server": "~7.0",
    "@angular/router": "~7.0",
    "@angular/upgrade": "~7.0",
    "@types/core-js": "~2.5",
    "@uirouter/angularjs": "^0.3.0",
    "angular": "1.6.10",
    "angular-animate": "~1.5.0",
    "angular-cache": "^4.6.0",
    "angular-messages": "1.5.x",
    "angular-sanitize": "1.5.x",
    "angular-translate": "^2.15.1",
    "angular-translate-loader-partial": "^2.15.1",
    "angular-translate-loader-url": "^2.15.1",
    "angular-ui-bootstrap": "^1.3.3",
    "bootstrap": "4.3.1",
    "core-js": "~2.5",
    "g": "^2.0.1",
    "lodash": "^4.17.11",
    "ngma": "^1.0.6",
